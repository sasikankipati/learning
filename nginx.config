events{}
http {
    include /etc/nginx/mime.types;
    server {
        listen 80;
        server_name localhost;
        #root /usr/share/nginx/html;
        index index.html;
        location /api/authenticate {
          proxy_pass http://learning-app:8090/api/authenticate;
        }
        location / {
            try_files $uri $uri/ /index.html;
        }
    }
}
